page {
  background-color: #f4f4f4;
  padding: 0rpx 0rpx 72rpx 0rpx;
}

.section {
  background-color: #fff;
  padding: 0 20rpx;
}

.banner {
  width: 100%;
  height: 320rpx;
  background-color: #9a9a9a;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}

.banner>image {
  width: 100%;
  height: 100%;
}

.iptList .item {
  height: 78rpx;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #000000;
  border-bottom: 1rpx solid #cfcfcf;
}

.iptList .item:last-child {
  border-bottom: none;
}

.selectBox {
  height: 90rpx;
  padding: 0 20rpx;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #000000;
}

.selectBox radio {
  zoom: 0.8;
  margin-right: 18rpx;
}
.selectBox .item > image{
  width: 20rpx;
  height: 20rpx;
  margin-right: 18rpx;
}
.ruleBox {
  font-family: MicrosoftYaHei;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 30rpx;
  letter-spacing: 0px;
  color: #999999;
  margin-top: 26rpx;
}

.btn {
  width: 100%;
  margin:0 auto;
  height: 69rpx;
  line-height: 69rpx;
  background-image: linear-gradient(-69deg,
      #d19b4e 0%,
      #ddbb81 100%);
  border-radius: 6rpx;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #ffffff;
  text-align: center;
  margin-top: 115rpx;
}